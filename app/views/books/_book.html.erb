<div id="<%= dom_id book %>">
  <p>
    <strong>Name:</strong>
    <%= book.name %>
  </p>

  <p>
    <strong>Book:</strong>
    <%= book.book_id %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= book.address %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= book.phone %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author_id %>
  </p>

</div>
